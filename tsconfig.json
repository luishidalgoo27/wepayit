{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "moduleResolution": "node",
      "strict": true,
      "jsx": "preserve",
      "sourceMap": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "lib": ["dom", "esnext"],
      "baseUrl": ".",
      "paths": {
        "@/*": ["resources/js/*"]
      }
    },
    "include": ["resources/js/**/*"],
    "exclude": ["node_modules"]
  }